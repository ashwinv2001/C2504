----------------------------------------create a school time table using table tag--------------------------------------------------
    <table border="1">
        <thead>
         <tr>
            <th colspan="6">TIMETABLE</th>
         </tr>
         <tr>
            <th>time</th>
            <th>monday</th>
            <th>tuesday</th>
            <th>Wednesday</th>
            <th>Thursday</th>
            <th>Friday</th>
        </tr>
        </thead>
        <tbody>
        <tr>
         <td>9:00-10:00</td>
         <td>English</td>
         <td>Maths</td>
         <td>Physics</td>
         <td>Chemistry</td>
         <td>PTE</td>
         </tr>
         <tr>
            <td>10:00-11:00</td>
            <td>Maths</td>
            <td>Biology</td>
            <td>PTE</td>
            <td>Maths</td>
            <td>English</td>
            </tr>
            <tr>
                <td>11:00-12:00</td>
                <td>Malayalam</td>
                <td>Chemistry</td>
                <td>Maths</td>
                <td>Physics</td>
                <td>EVS</td>
                </tr>
        </tbody>
        
    </table>
