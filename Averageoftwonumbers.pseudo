[TestClass]
public class CsvFileAccessTests
{
    [TestMethod]
    public void GetLastErrorMessage_ShouldReturnDefaultEmpty()
    {
        // Arrange
        var csvAccess = new CsvFileAccess("dummy.csv");

        // Act
        string err = csvAccess.GetLastErrorMessage();

        // Assert
        Assert.AreEqual(string.Empty, err);
    }
}
