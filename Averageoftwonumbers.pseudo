   public bool Close(int f_WaitForExitTimeOut)
        {
            this.process.CloseMainWindow();
            int l_WaitForExitTimeOut = f_WaitForExitTimeOut;
            if (0 > l_WaitForExitTimeOut)
            {
                l_WaitForExitTimeOut = CruciatusFactory.Settings.WaitForExitTimeout;
            }
            return this.process.WaitForExit(l_WaitForExitTimeOut);
        }
